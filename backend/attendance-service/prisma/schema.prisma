generator client {
  provider = "prisma-client"
  output = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "mysql"
}

model Attendance {
  id           String   @id @default(uuid())
  employee_id  String
  clock_in     DateTime
  clock_out    DateTime?
  photo_url    String?
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt

  @@index([employee_id])
}
