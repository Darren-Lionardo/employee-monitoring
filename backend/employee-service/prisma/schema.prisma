generator client {
  provider = "prisma-client"
  output = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "mysql"
}

model Employee {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  position  String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
